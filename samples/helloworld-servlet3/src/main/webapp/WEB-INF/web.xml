<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="false">

	<display-name>Pax Web Extender WAR Sample</display-name>

	<servlet>
		<servlet-name>duplicate</servlet-name>
		<servlet-class>org.ops4j.web.samples.helloworld.servlet3.internal.DuplicateDefinitionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>duplicate</servlet-name>
		<url-pattern>/duplicate</url-pattern>
	</servlet-mapping>

</web-app>
